#: ../lshw.cc:24
#, c-format
msgid "usage: %s [-format] [-options ...]\n"
msgstr "kullanım:% s [-format] [-seçenek …]\n"

#: ../lshw.cc:25
#, c-format
msgid "       %s -version\n"
msgstr "       %s sürümü\n"

#: ../lshw.cc:27
#, c-format
msgid "\t-version        print program version (%s)\n"
msgstr " -version     baskı programı sürümü (% s)\n"

#: ../lshw.cc:28
#, c-format
msgid ""
"\n"
"format can be\n"
msgstr ""
"\n"
"format olabilir\n"

#: ../lshw.cc:29
#, c-format
msgid "\t-html           output hardware tree as HTML\n"
msgstr " -html       donanım ağacını HTML olarak çıkar\n"

#: ../lshw.cc:30
#, c-format
msgid "\t-xml            output hardware tree as XML\n"
msgstr " -xml          XML olarak donanım ağacını çıktı\n"

#: ../lshw.cc:31
#, c-format
msgid "\t-short          output hardware paths\n"
msgstr " -kısa         çıktı donanım yolları\n"

#: ../lshw.cc:32
#, c-format
msgid "\t-businfo        output bus information\n"
msgstr " -businfo        çıkış veriyolu bilgisi\n"

#: ../lshw.cc:34
#, c-format
msgid "\t-X              use graphical interface\n"
msgstr " -X         grafik arayüz kullanır\n"

#: ../lshw.cc:35
#, c-format
msgid ""
"\n"
"options can be\n"
msgstr ""
"\n"
"seçenekler olabilir\n"

#: ../lshw.cc:37
#, c-format
msgid "\t-class CLASS    only show a certain class of hardware\n"
msgstr " -class CLASS       yalnızca belirli bir donanım sınıfını gösterir\n"

#: ../lshw.cc:38
#, c-format
msgid "\t-C CLASS        same as '-class CLASS'\n"
msgstr " -C CLASS,        ‘-class CLASS’ ile aynıdır\n"

#: ../lshw.cc:39
#, c-format
msgid "\t-c CLASS        same as '-class CLASS'\n"
msgstr " -c CLASS,         ‘-class CLASS’ ile aynıdır\n"

#: ../lshw.cc:41
#, c-format
msgid "\t-disable TEST   disable a test (like pci, isapnp, cpuid, etc. )\n"
msgstr ""
" -devre dışı TEST      bir testi devre dışı bırakır (pci, isapnp, cpuid "
"vb.)\n"

#: ../lshw.cc:43
#, c-format
msgid "\t-enable TEST    enable a test (like pci, isapnp, cpuid, etc. )\n"
msgstr " -enable TEST       bir testi etkinleştirir (pci, isapnp, cpuid vb.)\n"

#: ../lshw.cc:44
#, c-format
msgid "\t-quiet          don't display status\n"
msgstr " -sessiz       durumu görüntüleme\n"

#: ../lshw.cc:45
#, c-format
msgid ""
"\t-sanitize       sanitize output (remove sensitive information like serial "
"numbers, etc.)\n"
msgstr ""
" - çıktıları       sterilize edin (seri numaraları vb. gibi hassas bilgileri "
"kaldırın)\n"

#: ../lshw.cc:46
#, c-format
msgid "\t-numeric        output numeric IDs (for PCI, USB, etc.)\n"
msgstr " -numerik        çıkış sayısal kimlikleri (PCI, USB vb. için)\n"

