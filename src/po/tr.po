#: ../lshw.cc:24
#, c-format
msgid "usage: %s [-format] [-options ...]\n"
msgstr "kullanım: %s [-biçim] [-seçenekler ...]\n"

#: ../lshw.cc:25
#, c-format
msgid "       %s -version\n"
msgstr "       %s -sürüm\n"

#: ../lshw.cc:27
#, c-format
msgid "\t-version        print program version (%s)\n"
msgstr "\t-sürüm        yazdırma programı sürümü (%s)\n"

#: ../lshw.cc:28
#, c-format
msgid ""
"\n"
"format can be\n"
msgstr ""
"\n"
"biçim olabilir\n"

#: ../lshw.cc:29
#, c-format
msgid "\t-html           output hardware tree as HTML\n"
msgstr "\t-html           çıktı donanım ağacını HTML olarak\n"

#: ../lshw.cc:30
#, c-format
msgid "\t-xml            output hardware tree as XML\n"
msgstr "\t-xml            çıktı donanım ağacı XML olarak\n"

#: ../lshw.cc:31
#, c-format
msgid "\t-short          output hardware paths\n"
msgstr "\t-kısa          çıkış donanım yolları\n"

#: ../lshw.cc:32
#, c-format
msgid "\t-businfo        output bus information\n"
msgstr "\t-veri yolu        çıkış veri yolu bilgisi\n"

#: ../lshw.cc:34
#, c-format
msgid "\t-X              use graphical interface\n"
msgstr "\t-X              grafik arayüzü kullan\n"

#: ../lshw.cc:35
#, c-format
msgid ""
"\n"
"options can be\n"
msgstr ""
"\n"
"seçenekler olabilir\n"

#: ../lshw.cc:37
#, c-format
msgid "\t-class CLASS    only show a certain class of hardware\n"
msgstr "\t-sınıf SINIF    yalnızca belirli bir donanım sınıfını gösterir\n"

#: ../lshw.cc:38
#, c-format
msgid "\t-C CLASS        same as '-class CLASS'\n"
msgstr "\t-C SINIF        -sınıf SINIF ile aynı\n"

#: ../lshw.cc:39
#, c-format
msgid "\t-c CLASS        same as '-class CLASS'\n"
msgstr "\t-c SINIF        -sınıf SINIF ile aynı\n"

#: ../lshw.cc:41
#, c-format
msgid "\t-disable TEST   disable a test (like pci, isapnp, cpuid, etc. )\n"
msgstr ""
"\t-devre dışı TEST   bir testi devre dışı bırakır (pci, isapnp, cpuid, vb. "
"gibi)\n"

#: ../lshw.cc:43
#, c-format
msgid "\t-enable TEST    enable a test (like pci, isapnp, cpuid, etc. )\n"
msgstr ""
"\t-TEST i Etkinleştir    bir testi etkinleştirir (pci, isapnp, cpuid, vb. "
"gibi)\n"

#: ../lshw.cc:44
#, c-format
msgid "\t-quiet          don't display status\n"
msgstr "\t-sessiz          durumu gösterme\n"

#: ../lshw.cc:45
#, c-format
msgid ""
"\t-sanitize       sanitize output (remove sensitive information like serial "
"numbers, etc.)\n"
msgstr ""
"\t-sterilize       çıkışı sterilize edin (seri numaraları vb. gibi hassas "
"bilgileri kaldırın)\n"

#: ../lshw.cc:46
#, c-format
msgid "\t-numeric        output numeric IDs (for PCI, USB, etc.)\n"
msgstr "\t-sayısal        çıktı sayısal kimlikleri (PCI, USB vb. için)\n"

